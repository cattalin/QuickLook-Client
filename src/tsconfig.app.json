{
  "extends": "../tsconfig.json",
  "baseUrl": "src",
  "paths": {
    "@app/*": [
      "app/*"
    ],
    "@models/*": [
      "app/models/*"
    ],
    "@env/*": [
      "environments/*"
    ],
    "@assets/*": [
      "assets/*"
    ]
  },
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}